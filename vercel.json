{
    "version":2,
    "builds":[
        {
            "src":"greatkart.wsgi",
            "use":"@vercel/python",
            "config": { "maxlambdaSize":"60mb","runtime":"python3.9" }
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"greatkart.wsgi"
        }
    ]
}